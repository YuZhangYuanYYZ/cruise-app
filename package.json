    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "typescript": "^4.4.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
